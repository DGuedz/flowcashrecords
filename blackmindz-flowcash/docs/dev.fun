    { id: 'ethereum', name: 'Ethereum', color: '#627EEA', logo: <svg viewBox="0 0 32 32" className="w-8 h-8"><circle cx="16" cy="16" r="16" fill="#627EEA" /><g fill="#fff"><path d="M16.003 4L16 14.44l6.02-2.66L16.003 4z" fillOpacity="0.6" /><path d="M16.003 4L9.98 11.78l6.023 2.66V4z" /><path d="M16.003 19.16L16 26.956l6.024-8.47-6.021 0.674z" fillOpacity="0.6" /><path d="M16 26.956V19.16l-6.02-0.674L16 26.956z" /><path d="M16.003 17.78l6.02-2.66-6.02-2.73v5.39z" fillOpacity="0.2" /><path d="M9.98 15.12l6.023 2.66v-5.39L9.98 15.12z" fillOpacity="0.6" /></g></svg> },
    { id: 'bsc', name: 'BNB Chain', color: '#F3BA2F', logo: <svg viewBox="0 0 32 32" className="w-8 h-8"><circle cx="16" cy="16" r="16" fill="#F3BA2F" /><path d="M12.116 14.404L16 10.52l3.886 3.886 2.26-2.26L16 6l-6.144 6.144 2.26 2.26zM6 16l2.26-2.26L10.52 16l-2.26 2.26L6 16zm6.116 1.596L16 21.48l3.886-3.886 2.26 2.26L16 26l-6.144-6.144-2.26-2.26zm9.764-5.596L26 16l-2.26 2.26L21.48 16l2.26-2.26zm-3.764 1.596L16 15.48l-2.116-2.116L16 11.48l2.116 2.116z" fill="#fff" /></svg> },
    { id: 'arbitrum', name: 'Arbitrum', color: '#28A0F0', logo: <svg viewBox="0 0 32 32" className="w-8 h-8"><circle cx="16" cy="16" r="16" fill="#28A0F0" /><path d="M16.6 7.2l7.8 13.5c.2.3.2.7 0 1l-3.9 6.8c-.2.3-.5.5-.9.5H12.4c-.4 0-.7-.2-.9-.5l-3.9-6.8c-.2-.3-.2-.7 0-1L15.4 7.2c.2-.3.5-.5.9-.5h.3c.4 0 .7.2.9.5zM16 11.5L19.5 18h-7L16 11.5zm-6.5 8.5h3l1.5 2.6 1.5-2.6h3L16 23.1 13.5 20z" fill="#fff" /></svg> },
  ];

  const addLogEntry = (text, status, delay) => {
    return new Promise(resolve => {
      setTimeout(() => {
        setLog(prev => [...prev, { text, status, timestamp: new Date().toLocaleTimeString() }]);
        resolve();
      }, delay);
    });
  };

  const updateEstimatedFees = () => {
    // Simulate different gas fees for different networks
    const baseFee = selectedNetwork === 'ethereum' ? 0.015 : selectedNetwork === 'arbitrum' ? 0.003 : 0.005; // ETH equivalent
    const wormholeFee = 0.1; // Fixed Wormhole fee in SOL
    setEstimatedFees({
      wormhole: wormholeFee,
      gas: baseFee,
      total: wormholeFee * 200 + baseFee * 180, // Assuming SOL at $200 and ETH at $1800 for total USD estimate
    });
  };

  useEffect(() => {
    updateEstimatedFees();
  }, [selectedNetwork]);

  const handleBridgeAssets = async () => {
    if (((assetToBridge === 'chains' || assetToBridge === 'usdc') && (!bridgeAmount || parseFloat(bridgeAmount) <= 0)) || (assetToBridge === 'nft' && !bridgeAmount)) {
      setLog([{ text: '[ERRO] Insira um valor válido para a travessia do wormhole.', status: 'error', timestamp: new Date().toLocaleTimeString() }]);
      return;
    }

    setIsProcessing(true);
    setBridgeStep('locking');
    setLog([]); // Clear logs for new bridge operation

    const amount = assetToBridge === 'nft' ? '1 NFT "Correntes"' : assetToBridge === 'usdc' ? `${bridgeAmount} USDC` : `${bridgeAmount} $CHAINS`;
    const fromNetwork = bridgeDirection === 'solana-to-evm' ? 'Solana' : 'EVM';
    const toNetwork = networks.find(n => n.id === selectedNetwork)?.name;

    await addLogEntry(`[CULTURAL BRIDGE] Iniciando travessia musical de ${amount}...`, 'pending', 500);
    await addLogEntry(`De ${fromNetwork} para ${toNetwork} - A revolução se espalha!`, 'pending', 800);
    await addLogEntry(`[LOCK] Ativos culturais sendo depositados no Portal Wormhole...`, 'pending', 1200);

    setBridgeStep('signing');
    await addLogEntry(`[GUARDIANS] Guardiões detectando travessia cross-chain (3/19)...`, 'pending', 1000);
    await addLogEntry(`[GUARDIANS] Mais guardiões se conectando à rede (9/19)...`, 'pending', 1000);
    await addLogEntry(`[GUARDIANS] Consensus alcançado! (13/19)`, 'pending', 1000);

    setBridgeStep('vaa');
    const vaaHash = `0x${[...Array(10)].map(() => Math.floor(Math.random() * 16).toString(16)).join('')}`;
    await addLogEntry(`[VAA] Verified Action Approval criado! Hash: ${vaaHash}...`, 'success', 1500);
    await addLogEntry(`[WORMHOLE] Portal aberto entre as realidades blockchain...`, 'pending', 800);

    setBridgeStep('relaying');
    await addLogEntry(`[RELAY] Relayers carregando ativos através do wormhole...`, 'pending', 1200);
    await addLogEntry(`[GAS] Estimando gas na rede ${toNetwork}: ${estimatedFees.gas} ETH`, 'pending', 800);

    setBridgeStep('executing');
    await addLogEntry(`[EXEC] Materializando ativos na rede ${toNetwork}...`, 'pending', 2000);
    await addLogEntry(`[SUCCESS] Travessia concluída! ${amount} agora pulsam em ${toNetwork}!`, 'success', 500);
    await addLogEntry(`[IMPACT] A música brasileira conquista mais uma blockchain!`, 'success', 300);

    setBridgeStep('complete');
    setIsProcessing(false);
    setBridgeAmount('');
    setTimeout(() => setBridgeStep('prepare'), 3000); // Reset UI after a short delay
  };

  const handleCrossChainClaim = async () => {
    setIsProcessing(true);
    setLog([]);

    await addLogEntry(`[INIT] Iniciando claim cross-chain de recompensas USDC...`, 'pending', 500);
    await addLogEntry(`[VERIFY] Verificando posse do NFT "Correntes" na rede EVM...`, 'pending', 1500);
    await addLogEntry(`[MSG] Contrato Satélite enviando mensagem via Wormhole para Solana...`, 'pending', 2000);
    await addLogEntry(`[SIGN] Aguardando VAA da mensagem de claim (7/19)...`, 'pending', 1500);
    await addLogEntry(`[VAA] VAA de claim gerado. Hash: 0x${[...Array(10)].map(() => Math.floor(Math.random() * 16).toString(16)).join('')}...`, 'success', 1500);
    await addLogEntry(`[RELAY] Relayer entregando VAA ao contrato RoyaltyDistributor na Solana...`, 'pending', 1000);
    await addLogEntry(`[PROCESS] RoyaltyDistributor.process_cross_chain_claim() executado. Verificando VAA...`, 'pending', 2000);
    await addLogEntry(`[AUTH] VAA autêntico. Calculando recompensas: $24.80 USDC...`, 'success', 1000);
    await addLogEntry(`[BRIDGE_BACK] Iniciando ponte de USDC de volta para a rede de origem via Wormhole...`, 'pending', 1500);
    await addLogEntry(`[SUCCESS] Claim concluído! $24.80 USDC creditado na sua carteira EVM.`, 'success', 2000);

    setIsProcessing(false);
  };

  return (
    <div className="space-y-6 max-w-4xl">
      <div className="flex items-center gap-3 mb-6">
        <div className="p-3 rounded-lg bg-[#85BB65]/20">
          <BridgeIcon className="w-6 h-6 text-[#85BB65]" />
        </div>
        <div>
          <h1 className={`text-2xl font-bold ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>
            Ponte Multi-Chain Wormhole
          </h1>
          <p className={`text-sm ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'}`}>
            Infraestrutura cross-chain universal para NFTs e tokens
          </p>
        </div>
      </div>

      {/* Tabs for Tutorial, Bridge, Claims */}
      <div className={`flex gap-1 p-1 ${isDark ? 'bg-[#3E3E3C]' : 'bg-[#85BB65]/10'} border border-[#85BB65]/30 rounded-lg`}>
        <button onClick={() => setActiveTab('tutorial')} className={`flex-1 px-3 py-2 text-sm rounded transition-all ${activeTab === 'tutorial' ? 'bg-[#85BB65] text-[#3E3E3C] font-medium' : `${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} hover:text-[#85BB65]`}`}>
          <div className="flex items-center gap-2 justify-center">
            <FileTextIcon className="w-4 h-4" />
            <span>Tutorial</span>
          </div>
        </button>
        <button onClick={() => setActiveTab('assets')} className={`flex-1 px-3 py-2 text-sm rounded transition-all ${activeTab === 'assets' ? 'bg-[#85BB65] text-[#3E3E3C] font-medium' : `${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} hover:text-[#85BB65]`}`}>
          <div className="flex items-center gap-2 justify-center">
            <SwapIcon className="w-4 h-4" />
            <span>Bridge</span>
          </div>
        </button>
        <button onClick={() => setActiveTab('claims')} className={`flex-1 px-3 py-2 text-sm rounded transition-all ${activeTab === 'claims' ? 'bg-[#85BB65] text-[#3E3E3C] font-medium' : `${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} hover:text-[#85BB65]`}`}>
          <div className="flex items-center gap-2 justify-center">
            <CheckCircleIcon className="w-4 h-4" />
