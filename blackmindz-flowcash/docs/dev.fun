                <button onClick={() => setAssetToBridge('chains')} className={`px-3 py-2 text-sm rounded transition-all ${assetToBridge === 'chains' ? 'bg-[#85BB65] text-[#3E3E3C] font-medium' : `${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} hover:text-[#85BB65]`}`}>
                  <div className="flex items-center gap-2 justify-center">
