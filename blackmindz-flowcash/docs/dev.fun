    if (!userWallet) {
      setStatus('Conecte sua carteira para verificar status');
      return;
    }
    setIsCheckingDiscount(true);
    try {
      const connection = new Connection('https://rpc.dev.fun/c85be3993acd67cde661');
      const tokenMintPubkey = new PublicKey(tokenMint);
      const walletPubkey = new PublicKey(userWallet);
      const tokenAccounts = await connection.getTokenAccountsByOwner(walletPubkey, {
        mint: tokenMintPubkey
      });
      if (tokenAccounts.value.length > 0) {
        const balance = await connection.getTokenAccountBalance(tokenAccounts.value[0].pubkey);
        const tokenBalance = parseFloat(balance.value.amount) / Math.pow(10, balance.value.decimals);
        if (tokenBalance > 0) {
          setHasDiscount(true);
          setStatus(t('patronActive'));
        } else {
          setHasDiscount(false);
          setStatus(t('patronInactive'));
        }
      } else {
        setHasDiscount(false);
        setStatus(t('patronInactive'));
      }
      setDiscountChecked(true);
    } catch (error) {
      console.error('Error checking token balance:', error);
      setStatus('Erro ao verificar saldo. Tente novamente.');
    } finally {
      setIsCheckingDiscount(false);
    }
  };
  const handleEndorse = async () => {
    if (!userWallet) {
      setStatus('Por favor, conecte sua carteira primeiro');
      return;
