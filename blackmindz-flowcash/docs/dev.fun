function ClaimsWidget() {
  const { isDark } = useTheme();
  const [isProcessing, setIsProcessing] = useState(false);
  const [status, setStatus] = useState('');
  const [claimedAmount, setClaimedAmount] = useState(0);
  const availableAmount = 8.20;

  const handleClaim = async () => {
    const amountToClaim = availableAmount - claimedAmount;
    if (amountToClaim <= 0 || isProcessing) return;

    setIsProcessing(true);
    setStatus('Conectando ao contrato...');
    await new Promise(resolve => setTimeout(resolve, 1000));
    setStatus('Processando resgate...');
    await new Promise(resolve => setTimeout(resolve, 2000));
    setClaimedAmount(prev => prev + amountToClaim);
    const finalAmount = amountToClaim * 0.9; // Simulate 10% fee
    setStatus(`${finalAmount.toFixed(2)} USDC resgatados!`);
    setIsProcessing(false);
    setTimeout(() => setStatus(''), 5000);
  };

  const currentAvailable = availableAmount - claimedAmount;

  return (
    <div className={`${isDark ? 'bg-[#3E3E3C]' : 'bg-[#85BB65]/8'} border border-[#85BB65]/20 rounded-lg p-6 mb-6`}>
      <h3 className={`text-lg font-bold ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} mb-4`}>Recompensas Disponíveis</h3>
      <div className="text-center mb-6">
        <div className="text-3xl font-bold text-[#85BB65] mb-2">${currentAvailable.toFixed(2)}</div>
        <div className={`text-sm ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'} mb-4`}>
          {currentAvailable > 0 ? 'Pronto para resgate' : 'Nenhuma recompensa disponível'}
        </div>

        <div className="grid grid-cols-2 gap-3 mb-4">
          <div className="bg-[#85BB65]/10 border border-[#85BB65]/20 rounded-lg p-3 text-center">
            <div className="text-lg font-bold text-[#85BB65]">$4.85</div>
            <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'}`}>Royalties</div>
          </div>
          <div className="bg-[#85BB65]/10 border border-[#85BB65]/20 rounded-lg p-3 text-center">
            <div className="text-lg font-bold text-[#85BB65]">$3.35</div>
            <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'}`}>Juros DeFi</div>
          </div>
        </div>

        <div className="mb-4 pt-3 border-t border-[#85BB65]/20">
          <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/40' : 'text-[#3E3E3C]/40'} mb-2`}>Origem dos Royalties (Setembro)</div>
          <div className="space-y-1 text-xs">
            <div className={`flex justify-between ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'}`}>
              <span>Spotify:</span>
              <span className={`${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>$2.15</span>
            </div>
            <div className={`flex justify-between ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'}`}>
              <span>Apple Music:</span>
              <span className={`${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>$1.89</span>
            </div>
            <div className={`flex justify-between ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'}`}>
              <span>Outras:</span>
              <span className={`${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>$0.81</span>
            </div>
          </div>
        </div>
      </div>

      <button
        onClick={handleClaim}
        disabled={isProcessing || currentAvailable <= 0}
        className="w-full bg-[#85BB65] hover:bg-[#228D57] disabled:bg-[#85BB65]/50 disabled:cursor-not-allowed text-[#3E3E3C] font-medium py-3 px-4 rounded-lg transition-all duration-200 flex items-center justify-center gap-2"
      >
        {isProcessing ? (
          <>
            <div className="animate-spin rounded-full h-4 w-4 border-b-2 border-[#3E3E3C]"></div>
            <span>Processando...</span>
          </>
        ) : (
          <>
            <CheckCircleIcon className="w-4 h-4" />
            <span>{currentAvailable > 0 ? 'Resgatar Recompensas' : 'Recompensas Resgatadas'}</span>
          </>
        )}
      </button>
      <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/40' : 'text-[#3E3E3C]/40'} mt-3 text-center`}>
        Taxa de 10% • Processamento automático
      </div>
      {status && (
        <div className={`text-center text-xs p-2 rounded-lg mt-4 ${status.includes('Sucesso') ? 'bg-[#85BB65]/20 text-[#85BB65]' : 'bg-[#85BB65]/20 text-[#85BB65]'}`}>
          {status}
        </div>
      )}
    </div>
  );
}

