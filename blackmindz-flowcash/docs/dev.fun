            <div>
              <label className={`block text-sm font-medium ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} mb-2`}>
                {assetToBridge === 'nft' ? 'ID do NFT' : 'Quantidade'}
