          <div key={tx.id} className="p-4 hover:bg-[#85BB65]/5 transition-colors">
            <div className="flex items-start justify-between gap-4">
              <div className="flex items-start gap-3 flex-1">
                <div className="mt-0.5">
                  {getTypeIcon(tx.type)}
                </div>
                <div className="flex-1 min-w-0">
                  <div className={`font-medium ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} mb-1`}>
                    {tx.description}
                  </div>
                  <div className={`text-sm ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'} mb-2`}>
                    {tx.details}
                  </div>
                  <div className="flex flex-col sm:flex-row sm:items-center gap-2">
                    <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/40' : 'text-[#3E3E3C]/40'}`}>
                      {tx.date}
                    </div>
                    <div className="flex items-center gap-2">
                      <span className={`text-xs px-2 py-1 rounded ${getTypeColor(tx.type)} bg-current/10`}>
                        {tx.status}
                      </span>
                      <button onClick={() => window.open(`https://solscan.io/tx/${tx.txHash}`, '_blank')} className={`text-xs ${isDark ? 'text-[#E8E9C9]/40 hover:text-[#85BB65]' : 'text-[#3E3E3C]/40 hover:text-[#85BB65]'} transition-colors flex items-center gap-1`}>
                        Ver TX <ExternalLinkIcon className="w-3 h-3" />
                      </button>
                    </div>
                  </div>
                </div>
              </div>
              <div className="text-right">
                <div className={`font-medium ${tx.amount.startsWith('+') ? 'text-[#85BB65]' : tx.amount.startsWith('-') && tx.amount !== '-' ? 'text-red-400' : isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>
                  {tx.amount}
                </div>
              </div>
            </div>
          </div>
        ))}
      </div>

      {filteredTransactions.length === 0 && (
        <div className="text-center py-12">
          <CalendarIcon className={`w-16 h-16 ${isDark ? 'text-[#E8E9C9]/40' : 'text-[#3E3E3C]/40'} mx-auto mb-4`} />
          <p className={`${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'}`}>
            Nenhuma transação encontrada para o filtro selecionado
          </p>
        </div>
      )}
    </div>
  );
}

function BridgeSection() {
