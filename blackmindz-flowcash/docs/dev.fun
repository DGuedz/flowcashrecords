function ErrorBoundary({ children }) {
  const [hasError, setHasError] = useState(false);
  const [error, setError] = useState(null);
  const { isDark } = useTheme();

  useEffect(() => {
    const handleError = (error) => {
      console.error('Application error:', error);
      if (error.message && error.message.includes('MetaMask')) {
        setError('MetaMask detectado! Este app usa wallets Solana. Por favor, instale Phantom ou Solflare.');
        setHasError(true);
      } else if (error.message && (error.message.includes('CORS') || error.message.includes('origins don\'t match') || error.message.includes('Failed to fetch') || error.message.includes('NetworkError'))) {
        console.warn('CORS/Network error detected, using fallback data');
        // For CORS/Network errors, we might want to just log and continue, or show a less intrusive message
        // For now, we'll just log and let the app try to render with potentially missing data.
        // If the user wants a hard error for these, we can adjust.
        return;
      }
      // Only set hasError for critical, unrecoverable errors like wallet incompatibility
      // For network errors, the app might still function with cached/default data
      // setHasError(true); 
    };

    const handleUnhandledRejection = (event) => {
      if (event.reason?.message?.includes('CORS') || event.reason?.message?.includes('origins don\'t match') || event.reason?.message?.includes('Failed to fetch') || event.reason?.message?.includes('NetworkError') || event.reason?.message?.includes('dev.fun') || event.reason?.toString?.().includes('CORS')) {
        console.warn('CORS/Network promise rejection detected, using fallback data');
        event.preventDefault(); // Prevent default browser error logging for these specific rejections
        return;
      }
      console.error('Unhandled promise rejection:', event.reason);
      // setHasError(true); // Only set hasError for critical rejections
    };

    window.addEventListener('error', handleError);
    window.addEventListener('unhandledrejection', handleUnhandledRejection);

    return () => {
      window.removeEventListener('error', handleError);
      window.removeEventListener('unhandledrejection', handleUnhandledRejection);
    };
  }, []);

  if (hasError) {
    return (
      <div className={`min-h-screen ${isDark ? 'bg-[#3E3E3C] text-[#E8E9C9]' : 'bg-white text-[#3E3E3C]'} flex items-center justify-center`}>
        <div className={`max-w-md text-center p-6 ${isDark ? 'bg-[#3E3E3C]' : 'bg-[#E8E9C9]'} border border-red-500/30 rounded-lg`}>
          <div className="w-16 h-16 bg-red-500/20 rounded-full flex items-center justify-center mx-auto mb-4">
            <XIcon className="w-8 h-8 text-red-400" />
          </div>
          <h2 className="text-xl font-bold mb-2 text-red-400">Erro de Wallet</h2>
          <p className={`text-sm ${isDark ? 'text-[#E8E9C9]/80' : 'text-[#3E3E3C]/80'} mb-4`}>{error}</p>
          <div className={`space-y-2 text-xs ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'} mb-4`}>
            <p>Wallets recomendadas para Solana:</p>
            <div className="flex justify-center gap-4">
              <span>• Phantom</span>
              <span>• Solflare</span>
              <span>• Backpack</span>
            </div>
          </div>
          <button onClick={() => setHasError(false)} className={`px-4 py-2 bg-[#85BB65] hover:bg-[#228D57] ${isDark ? 'text-[#3E3E3C]' : 'text-white'} font-medium rounded-lg transition-colors`}>
            Tentar Novamente
          </button>
        </div>
      </div>
    );
  }

  return children;
}

