                    className={`p-4 rounded-lg border transition-all duration-300 relative group ${selectedNetwork === network.id ? 'bg-[#85BB65]/20 border-[#85BB65] scale-105' : `border-[#85BB65]/30 ${isDark ? 'hover:bg-[#85BB65]/10' : 'hover:bg-[#85BB65]/5'} hover:scale-102`}`}
                  >
                    <div className="flex flex-col items-center gap-2">
                      <div className="relative">
                        {network.logo}
