                <button onClick={() => setAssetToBridge('nft')} className={`px-3 py-2 text-sm rounded transition-all ${assetToBridge === 'nft' ? 'bg-[#85BB65] text-[#3E3E3C] font-medium' : `${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} hover:text-[#85BB65]`}`}>
