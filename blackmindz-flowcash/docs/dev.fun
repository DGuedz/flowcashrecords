                <button onClick={() => setBridgeDirection('evm-to-solana')} className={`px-3 py-2 text-sm rounded transition-all ${bridgeDirection === 'evm-to-solana' ? 'bg-[#85BB65] text-[#3E3E3C] font-medium' : `${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} hover:text-[#85BB65]`}`}>EVM → Solana</button>
              </div>
            </div>

            <div className="mb-6">
              <label className={`block text-sm font-medium ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} mb-2`}>
