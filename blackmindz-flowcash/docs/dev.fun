function ClaimsSection() {
  const { isDark } = useTheme();
  const [isProcessing, setIsProcessing] = useState(false);
  const [status, setStatus] = useState('');
  const [claimedAmount, setClaimedAmount] = useState(0); // Track claimed amount for simulation
  const availableAmount = 8.20; // Total available for claim

  const [testnetLogs, setTestnetLogs] = useState([]);

  const addTestnetLog = (message, type = 'info') => {
    const timestamp = new Date().toLocaleTimeString();
    setTestnetLogs(prev => [...prev.slice(-4), { message, type, timestamp }]);
  };

  const handleClaim = async () => {
    const amountToClaim = availableAmount - claimedAmount;
    if (amountToClaim <= 0 || isProcessing) return;

    setIsProcessing(true);
    setTestnetLogs([]); // Clear logs for new claim

    try {
      addTestnetLog('[TESTNET] Iniciando processo de claim de recompensas...', 'info');
      setStatus('Preparando claim...');
      await new Promise(resolve => setTimeout(resolve, 800));

      addTestnetLog('[TESTNET] Verificando elegibilidade do NFT "Correntes"...', 'info');
      await new Promise(resolve => setTimeout(resolve, 1000));
      addTestnetLog('[TESTNET] ✓ 125 NFTs encontrados na carteira', 'success');

      addTestnetLog('[TESTNET] Calculando recompensas acumuladas...', 'info');
      await new Promise(resolve => setTimeout(resolve, 1200));
      addTestnetLog(`[TESTNET] Royalties disponíveis: ${(amountToClaim * 0.6).toFixed(2)} USDC`, 'info');
      addTestnetLog(`[TESTNET] Juros DeFi disponíveis: ${(amountToClaim * 0.4).toFixed(2)} USDC`, 'info');

      setStatus('Conectando ao contrato RoyaltyDistributor...');
      addTestnetLog('[TESTNET] Conectando ao smart contract na Solana...', 'info');
      await new Promise(resolve => setTimeout(resolve, 1000));

      addTestnetLog('[TESTNET] Executando função processClaim()...', 'info');
      setStatus('Aguardando assinatura da carteira...');
      await new Promise(resolve => setTimeout(resolve, 1500));

      addTestnetLog('[TESTNET] Transação assinada! Processando na blockchain...', 'success');
      setStatus('Processando resgate...');
      await new Promise(resolve => setTimeout(resolve, 1800));

      addTestnetLog('[TESTNET] Resgatando fundos do protocolo Kamino Lend...', 'info');
      await new Promise(resolve => setTimeout(resolve, 1000));

      addTestnetLog('[TESTNET] Aplicando taxa de protocolo (10%)...', 'info');
      await new Promise(resolve => setTimeout(resolve, 800));

      const finalAmount = amountToClaim * 0.9; // Simulate 10% fee
      addTestnetLog(`[TESTNET] Valor final: ${finalAmount.toFixed(2)} USDC`, 'success');
      addTestnetLog('[TESTNET] Transferindo USDC para carteira...', 'success');
      await new Promise(resolve => setTimeout(resolve, 1200));

      setClaimedAmount(prev => prev + amountToClaim); // Update claimed amount
      addTestnetLog('[TESTNET] ✓ Claim concluído com sucesso!', 'success');
      setStatus(`Sucesso! ${finalAmount.toFixed(2)} USDC resgatados para sua carteira.`);
    } catch (error) {
      addTestnetLog('[TESTNET] Erro no processo de claim', 'error');
      setStatus('Erro no claim. Tente novamente.');
    } finally {
      setIsProcessing(false);
      setTimeout(() => setStatus(''), 8000); // Clear status after a few seconds
    }
  };

  const currentAvailable = availableAmount - claimedAmount;

  return (
    <div className="space-y-6">
      <div className="flex flex-col sm:flex-row sm:items-center justify-between gap-4">
        <h1 className={`text-2xl font-bold ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>Claims & Recompensas</h1>
