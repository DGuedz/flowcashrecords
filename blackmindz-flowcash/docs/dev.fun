function WormholeProvider({
  children
}) {
  const [evmWallet, setEvmWallet] = useState(null);
  const [evmProvider, setEvmProvider] = useState(null);
  const [evmChainId, setEvmChainId] = useState(null);
  const [isEVMConnected, setIsEVMConnected] = useState(false);
  const supportedEVMNetworks = [{
    id: 137,
    name: 'Polygon',
    symbol: 'MATIC',
    rpc: 'https://polygon-rpc.com'
  }, {
    id: 1,
    name: 'Ethereum',
    symbol: 'ETH',
    rpc: 'https://eth.llamarpc.com'
  }, {
    id: 56,
    name: 'BNB Chain',
    symbol: 'BNB',
    rpc: 'https://bsc-dataseed.binance.org'
  }, {
    id: 42161,
    name: 'Arbitrum',
    symbol: 'ETH',
    rpc: 'https://arb1.arbitrum.io/rpc'
  }];
  const connectEVMWallet = async () => {
    const isSandbox = typeof window !== 'undefined' && (window.location.hostname.includes('codesandbox.io') || window.location.hostname.includes('stackblitz.com') || window.location.hostname.includes('sandpack') || window.location.hostname.includes('csb.app') || window.location.hostname.includes('webcontainer') || window.parent !== window);
    const isDevFun = typeof window !== 'undefined' && window.location.hostname.includes('dev.fun');
    if (isSandbox || !isDevFun) {
      const demoAddress = '0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb';
      setEvmWallet(demoAddress);
      setIsEVMConnected(true);
      setEvmChainId(137);
      setEvmProvider(null);
      console.log('[SANDBOX MODE] Demo EVM wallet activated - Full functionality available on dev.fun');
      return;
    }
    if (window.ethereum) {
      try {
        const provider = window.ethereum;
        const accounts = await provider.request({
          method: 'eth_requestAccounts'
        });
        const chainId = await provider.request({
          method: 'eth_chainId'
        });
        setEvmWallet(accounts[0]);
        setEvmProvider(provider);
        setEvmChainId(parseInt(chainId, 16));
        setIsEVMConnected(true);
      } catch (error) {
        console.error('Error connecting to EVM wallet:', error);
        const demoAddress = '0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb';
        setEvmWallet(demoAddress);
        setIsEVMConnected(true);
        setEvmChainId(137);
        setEvmProvider(null);
        if (error.code === 4001) {
          console.log('Connection cancelled by user - Demo mode activated');
        } else if (error.code === -32002) {
          console.log('Pending request detected - Demo mode activated');
        } else {
          console.log('Connection error - Demo mode activated');
        }
      }
    } else {
      const demoAddress = '0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb';
      setEvmWallet(demoAddress);
      setIsEVMConnected(true);
      setEvmChainId(137);
      setEvmProvider(null);
      console.log('MetaMask not detected - Demo mode activated');
    }
  };
  const disconnectEVMWallet = () => {
    setEvmWallet(null);
    setEvmProvider(null);
    setEvmChainId(null);
    setIsEVMConnected(false);
  };
  const switchEVMNetwork = async targetChainId => {
    if (!evmProvider) {
      console.log('No EVM provider - Demo mode active');
      setEvmChainId(targetChainId);
      return;
    }
    const network = supportedEVMNetworks.find(n => n.id === targetChainId);
    if (!network) return;
    try {
      await evmProvider.request({
        method: 'wallet_switchEthereumChain',
        params: [{
          chainId: `0x${targetChainId.toString(16)}`
        }]
      });
      setEvmChainId(targetChainId);
    } catch (error) {
      console.error('Error switching network:', error);
      setEvmChainId(targetChainId);
    }
  };
  useEffect(() => {
    const isSandbox = typeof window !== 'undefined' && (window.location.hostname.includes('codesandbox.io') || window.location.hostname.includes('stackblitz.com') || window.location.hostname.includes('sandpack') || window.location.hostname.includes('csb.app') || window.location.hostname.includes('webcontainer') || window.parent !== window);
    if (isSandbox || !window.ethereum) {
      return;
    }
    let mounted = true;
    const handleAccountsChanged = accounts => {
      if (!mounted) return;
      if (accounts.length > 0) {
        setEvmWallet(accounts[0]);
      } else {
        disconnectEVMWallet();
      }
    };
    const handleChainChanged = chainId => {
      if (!mounted) return;
      setEvmChainId(parseInt(chainId, 16));
    };
    try {
      window.ethereum.on('accountsChanged', handleAccountsChanged);
      window.ethereum.on('chainChanged', handleChainChanged);
    } catch (error) {
      console.log('[EVM] Error setting up event listeners:', error);
    }
    return () => {
      mounted = false;
      if (!window.ethereum) return;
      try {
        if (typeof window.ethereum.removeListener === 'function') {
          window.ethereum.removeListener('accountsChanged', handleAccountsChanged);
          window.ethereum.removeListener('chainChanged', handleChainChanged);
        } else if (typeof window.ethereum.off === 'function') {
          window.ethereum.off('accountsChanged', handleAccountsChanged);
          window.ethereum.off('chainChanged', handleChainChanged);
        }
      } catch (error) {}
    };
  }, []);
  return <WormholeContext.Provider value={{
    evmWallet,
    evmProvider,
    evmChainId,
    connectEVMWallet,
    disconnectEVMWallet,
    isEVMConnected,
    switchEVMNetwork,
    supportedEVMNetworks
  }}>
      {children}
    </WormholeContext.Provider>;
}
