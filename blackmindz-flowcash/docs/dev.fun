function usePumpFunData(tokenMint) {
  const [tokenData, setTokenData] = useState(null);
  const [loading, setLoading] = useState(true);

  const fetchData = async () => {
    const isDev = window.location.hostname.includes('codesandbox.io') || window.location.hostname.includes('sandpack') || window.location.hostname.includes('csb.app') || window.location.hostname.includes('gitpod.io') || window.location.hostname === 'localhost' || window.location.hostname.includes('preview') || window.location.hostname.includes('stackblitz');
    if (isDev) {
      // Simulate data for dev environment
      setTokenData({
        contractAddress: tokenMint,
        symbol: 'CHAINS',
        name: '$CHAINS Token',
        price: 0.000045 + (Math.random() - 0.5) * 0.000005, // Simulate price fluctuation
        marketcap: 45000 + Math.random() * 10000, // Simulate market cap fluctuation
        priceChangeIn24h: 5.2 + (Math.random() - 0.5) * 10, // Simulate price change
        volumeIn24h: 12500 + Math.random() * 5000, // Simulate volume
      });
      setLoading(false);
      return;
    }

    try {
      const data = await fetchTokenInfoByAddress(tokenMint);
      setTokenData(data);
    } catch (error) {
      console.error('Error fetching token data:', error);
      // Fallback data if API fails
      setTokenData({
        contractAddress: tokenMint,
        symbol: 'CHAINS',
        name: '$CHAINS Token',
        price: 0.000045,
        marketcap: 45000,
        priceChangeIn24h: 5.2,
        volumeIn24h: 12500,
      });
    } finally {
      setLoading(false);
    }
  };

  useEffect(() => {
    fetchData();
    const interval = setInterval(fetchData, 30000); // Refresh every 30 seconds
    return () => clearInterval(interval);
  }, [tokenMint]);

  return { tokenData, loading };
}
