function LanguageProvider({
  children
}) {
  const [language, setLanguage] = useState('pt');
  const t = key => translations[language][key] || key;
  return <LanguageContext.Provider value={{
    language,
    setLanguage,
    t
  }}>
      {children}
    </LanguageContext.Provider>;
}
