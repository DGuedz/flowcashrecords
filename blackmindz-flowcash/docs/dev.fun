function EndorsementInterface({ tokenMint }) {
  const { userWallet } = useDevapp();
  const { t } = useLanguage();
  const { isDark } = useTheme();

  const [solAmount, setSolAmount] = useState('');
  const [isLoading, setIsLoading] = useState(false);
  const [status, setStatus] = useState('');
  const [hasDiscount, setHasDiscount] = useState(false);
  const [isCheckingDiscount, setIsCheckingDiscount] = useState(false);
  const [discountChecked, setDiscountChecked] = useState(false);
  const [tokenData, setTokenData] = useState(null);

  const [purchaseMethod, setPurchaseMethod] = useState('wallet'); // 'wallet' or 'pix'
  const [pixAmount, setPixAmount] = useState('');
  const [, setShowPixQR] = useState(false); // Not used directly, but kept for context
  const [selectedProvider, setSelectedProvider] = useState('moonpay');
  const [conversionRate,] = useState(5.2); // BRL to USD conversion rate
  const [onRampStep, setOnRampStep] = useState('select'); // 'select', 'payment', 'processing', 'complete'
  const [estimatedCrypto, setEstimatedCrypto] = useState({ usdc: 0, sol: 0 });
  const [selectedCrypto, setSelectedCrypto] = useState('usdc'); // 'usdc' or 'sol'

  const [testnetLogs, setTestnetLogs] = useState([]);
  const [isTestnetMode] = useState(true); // Always true for testnet simulation

  const addTestnetLog = (message, type = 'info') => {
    const timestamp = new Date().toLocaleTimeString();
    setTestnetLogs(prev => [...prev.slice(-4), { message, type, timestamp }]);
  };

  const checkDiscountEligibility = async () => {
    if (!userWallet) {
      setStatus('Conecte sua carteira para verificar status');
      return;
    }
    setIsCheckingDiscount(true);
    addTestnetLog('[TESTNET] Iniciando verificação de status de holder cultural...', 'info');
    try {
      addTestnetLog('[TESTNET] Conectando à RPC Solana Devnet...', 'info');
      await new Promise(resolve => setTimeout(resolve, 1000));
      addTestnetLog('[TESTNET] Buscando token accounts na carteira...', 'info');
      await new Promise(resolve => setTimeout(resolve, 1500));

      // Simulate token balance check
      const hasTokens = Math.random() > 0.3; // 70% chance of having tokens
      if (hasTokens) {
        addTestnetLog('[TESTNET] Token $CHAINS encontrado! Verificando saldo...', 'success');
        await new Promise(resolve => setTimeout(resolve, 1000));
        const simulatedBalance = Math.floor(Math.random() * 50000) + 1000; // Simulate a balance
        addTestnetLog(`[TESTNET] Saldo: ${simulatedBalance.toLocaleString()} $CHAINS`, 'success');
        setHasDiscount(true);
        setStatus(t('patronActive'));
        addTestnetLog('[TESTNET] Status: HOLDER CULTURAL ATIVO - 50% de desconto habilitado!', 'success');
      } else {
        addTestnetLog('[TESTNET] Nenhum token $CHAINS encontrado na carteira', 'warning');
        setHasDiscount(false);
        setStatus(t('patronInactive'));
      }
      setDiscountChecked(true);
    } catch (error) {
      console.error('Error checking token balance:', error);
      addTestnetLog('[TESTNET] Erro na verificação. Tentando novamente...', 'error');
      setStatus('Erro ao verificar saldo. Tente novamente.');
    } finally {
      setIsCheckingDiscount(false);
    }
  };

  const handleEndorse = async () => {
    if (!userWallet) {
      setStatus('Por favor, conecte sua carteira primeiro');
      return;
    }
    if (!solAmount || parseFloat(solAmount) <= 0) {
      setStatus('Por favor, insira um valor válido de SOL');
      return;
    }

    setIsLoading(true);
    setTestnetLogs([]); // Clear logs for new transaction

    try {
      addTestnetLog('[TESTNET] Iniciando compra de $CHAINS na bonding curve...', 'info');
      setStatus('Preparando transação...');
      await new Promise(resolve => setTimeout(resolve, 1000));

      addTestnetLog(`[TESTNET] Valor: ${solAmount} SOL → Calculando $CHAINS...`, 'info');
      await new Promise(resolve => setTimeout(resolve, 800));
      const estimatedTokens = Math.floor(parseFloat(solAmount) * 200000 * (0.9 + Math.random() * 0.2)); // Simulate token amount
      addTestnetLog(`[TESTNET] Estimativa: ${estimatedTokens.toLocaleString()} $CHAINS`, 'info');

      setStatus('Aguardando assinatura da carteira...');
      addTestnetLog('[TESTNET] Aguardando aprovação na carteira Phantom/Solflare...', 'warning');
      await new Promise(resolve => setTimeout(resolve, 2000));

      addTestnetLog('[TESTNET] Transação assinada! Enviando para a rede...', 'success');
      setStatus('Processando na blockchain...');
      await new Promise(resolve => setTimeout(resolve, 1500));

      const txHash = `0x${[...Array(16)].map(() => Math.floor(Math.random() * 16).toString(16)).join('')}`;
      addTestnetLog(`[TESTNET] TX Hash: ${txHash}`, 'info');
      addTestnetLog('[TESTNET] Aguardando confirmação da rede (1/3)...', 'info');
      await new Promise(resolve => setTimeout(resolve, 1000));
      addTestnetLog('[TESTNET] Confirmação recebida (3/3) ✓', 'success');

      setStatus(`Sucesso! ${estimatedTokens.toLocaleString()} $CHAINS adquiridos!`);
      addTestnetLog('[TESTNET] Tokens $CHAINS creditados na carteira!', 'success');
      setSolAmount('');
      fetchTokenData(); // Refresh token data after purchase
    } catch (error) {
      addTestnetLog('[TESTNET] Erro na transação. Verifique sua carteira.', 'error');
      setStatus('Erro na transação. Tente novamente.');
      console.error('Transaction error:', error);
    } finally {
      setIsLoading(false);
      setTimeout(() => setStatus(''), 8000); // Clear status after a few seconds
    }
  };

  const fetchTokenData = async () => {
    const isDev = window.location.hostname.includes('codesandbox.io') || window.location.hostname.includes('sandpack') || window.location.hostname.includes('csb.app') || window.location.hostname.includes('gitpod.io') || window.location.hostname === 'localhost' || window.location.hostname.includes('preview') || window.location.hostname.includes('stackblitz');
    if (isDev) {
      setTokenData({
        contractAddress: tokenMint,
        symbol: 'CHAINS',
        name: '$CHAINS Token',
        price: 0.000045 + (Math.random() - 0.5) * 0.000005, // Simulate price fluctuation
        marketcap: 45000 + Math.random() * 10000, // Simulate market cap fluctuation
        priceChangeIn24h: 5.2 + (Math.random() - 0.5) * 10, // Simulate price change
        volumeIn24h: 12500 + Math.random() * 5000, // Simulate volume
      });
      return;
    }
    try {
      const data = await fetchTokenInfoByAddress(tokenMint);
      setTokenData(data);
    } catch (error) {
      console.error('Error fetching token data:', error);
      // Fallback data if API fails
      setTokenData({
        contractAddress: tokenMint,
        symbol: 'CHAINS',
        name: '$CHAINS Token',
        price: 0.000045,
        marketcap: 45000,
        priceChangeIn24h: 5.2,
        volumeIn24h: 12500,
      });
    }
  };

  const calculateCryptoAmount = (brlAmount) => {
    if (!brlAmount || isNaN(brlAmount)) return { usdc: 0, sol: 0 };
    const usdAmount = parseFloat(brlAmount) / conversionRate;
    return {
      usdc: usdAmount * 0.98, // Simulate 2% fee
      sol: usdAmount * 0.98 / 180, // Simulate SOL price at $180
    };
  };

  const onRampProviders = [
    { id: 'moonpay', name: 'MoonPay', logo: MoonIcon, fees: '2.5%', time: '5-10 min', description: 'Líder global em on-ramp' },
    { id: 'ramp', name: 'Ramp Network', logo: SparklesIcon, fees: '2.2%', time: '3-8 min', description: 'Rápido e eficiente' },
    { id: 'transak', name: 'Transak', logo: RocketIcon, fees: '2.8%', time: '5-15 min', description: 'Foco em mercados emergentes' },
  ];

  useEffect(() => {
    fetchTokenData();
    const interval = setInterval(fetchTokenData, 30000); // Refresh every 30 seconds
    return () => clearInterval(interval);
  }, [tokenMint]);

  useEffect(() => {
    if (pixAmount) {
      setEstimatedCrypto(calculateCryptoAmount(pixAmount));
    }
  }, [pixAmount, conversionRate]);

  const handleOnRampPurchase = async () => {
    if (!pixAmount || parseFloat(pixAmount) < 50) {
      setStatus('Valor mínimo: R$ 50');
      return;
    }
    setOnRampStep('payment');
  };

  const simulateOnRampPayment = async () => {
    setOnRampStep('processing');
    setTestnetLogs([]); // Clear logs for new simulation

    addTestnetLog('[TESTNET] Iniciando simulação de pagamento PIX...', 'info');
    setStatus('Processando pagamento PIX...');
    await new Promise(resolve => setTimeout(resolve, 1000));

    addTestnetLog('[TESTNET] PIX detectado pelo provedor...', 'info');
    await new Promise(resolve => setTimeout(resolve, 1500));

    addTestnetLog('[TESTNET] Validando CPF e dados do pagador...', 'info');
    await new Promise(resolve => setTimeout(resolve, 1200));

    addTestnetLog('[TESTNET] Pagamento confirmado! Iniciando conversão BRL→USD...', 'success');
    setStatus('Pagamento confirmado! Convertendo para crypto...');
    await new Promise(resolve => setTimeout(resolve, 1000));

    addTestnetLog(`[TESTNET] Convertendo R$ ${pixAmount} → ${(parseFloat(pixAmount) / conversionRate).toFixed(2)} USD`, 'info');
    await new Promise(resolve => setTimeout(resolve, 1500));

    addTestnetLog(`[TESTNET] Comprando ${estimatedCrypto[selectedCrypto].toFixed(4)} ${selectedCrypto.toUpperCase()} no mercado...`, 'info');
    await new Promise(resolve => setTimeout(resolve, 1800));

    addTestnetLog('[TESTNET] Criando transação Solana para envio...', 'info');
    await new Promise(resolve => setTimeout(resolve, 1000));

    setOnRampStep('complete');
    addTestnetLog('[TESTNET] Crypto enviado com sucesso para carteira!', 'success');
    setStatus(`Sucesso! ${estimatedCrypto[selectedCrypto].toFixed(4)} ${selectedCrypto.toUpperCase()} enviado para sua carteira!`);

    setTimeout(() => {
      setOnRampStep('select');
      setShowPixQR(false);
      setPixAmount('');
      setTestnetLogs([]);
    }, 5000); // Reset after 5 seconds
  };


  return (
    <div className={`${isDark ? 'bg-[#3E3E3C]' : 'bg-[#85BB65]/8'} border border-[#85BB65]/20 rounded-lg p-4 sm:p-6`}>
      <div className="text-center mb-4 sm:mb-6">
        <h3 className={`text-lg sm:text-xl font-bold mb-2 ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} flex items-center justify-center gap-2`}>
          <div className="relative">
            <HeartIcon className="w-5 h-5 sm:w-6 sm:h-6 text-[#85BB65]" />
            <div className="absolute -top-1 -right-1 w-2 h-2 sm:w-3 sm:h-3 bg-[#85BB65] rounded-full animate-pulse"></div>
          </div>
          <span className="text-center leading-tight">Torne-se um Holder Cultural</span>
        </h3>
        <p className={`text-xs sm:text-sm ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>Escolha como participar</p>
        <p className="text-xs text-[#85BB65] mt-1 px-2">"Pensamento à frente. Revolução em andamento."</p>
      </div>

      {/* Purchase Method Selector */}
      <div className="mb-4 sm:mb-6">
        <div className={`grid grid-cols-2 gap-2 p-1 ${isDark ? 'bg-[#3E3E3C]' : 'bg-[#85BB65]/10'} border border-[#85BB65]/30 rounded-lg`}>
        <button onClick={() => setPurchaseMethod('wallet')} className={`px-3 py-2 text-sm rounded transition-all ${purchaseMethod === 'wallet' ? 'bg-[#85BB65] text-[#3E3E3C] font-medium' : `${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} hover:text-[#85BB65]`}`}>
              <div className="flex items-center gap-2">
                <WalletIcon className="w-4 h-4" />
                <span>Com Carteira</span>
              </div>
            </button>
            <button onClick={() => setPurchaseMethod('pix')} className={`px-3 py-2 text-sm rounded transition-all ${purchaseMethod === 'pix' ? 'bg-[#85BB65] text-[#3E3E3C] font-medium' : `${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'} hover:text-[#85BB65]`}`}>
              <div className="flex items-center gap-2">
                <svg className="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M3 10h18M7 15h1m4 0h1m-7 4h12a3 3 0 003-3V8a3 3 0 00-3-3H6a3 3 0 00-3 3v8a3 3 0 003 3z" /></svg>
                <span>Com PIX</span>
              </div>
            </button>
        </div>
        <p className={`text-xs ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'} mt-2 text-center`}>
          {purchaseMethod === 'wallet' ? 'Compra imediata on-chain. Recomendado para usuários cripto.' : 'Não tem carteira? Pague com PIX e receba $CHAINS automaticamente — sem aprender Web3.'}
        </p>
      </div>

      {/* Testnet Mode Indicator */}
      {isTestnetMode && (
        <div className="bg-orange-500/10 border border-orange-500/30 rounded-lg p-3 mb-4">
          <div className="flex items-center gap-2 mb-2">
            <div className="w-2 h-2 bg-orange-400 rounded-full animate-pulse"></div>
            <span className="text-orange-400 font-medium text-sm">MODO TESTNET ATIVO</span>
          </div>
          <p className="text-xs text-orange-400/80">
            Todas as transações são simuladas. Nenhum valor real será debitado.
          </p>
        </div>
      )}

      <div className="space-y-3 sm:space-y-4">
        {/* Discount Eligibility Check */}
        {userWallet && !discountChecked && (
          <div className={`${isDark ? 'bg-[#3E3E3C]' : 'bg-[#85BB65]/10'} border border-[#85BB65]/50 rounded-lg p-3 sm:p-4`}>
        <div className="flex items-center gap-2 mb-2">
              <ShieldCheckIcon className="w-4 h-4 sm:w-5 sm:h-5 text-[#85BB65] flex-shrink-0" />
              <span className="text-[#85BB65] font-medium text-sm sm:text-base">{t('verifyPatronTitle')}</span>
            </div>
            <p className={`text-xs sm:text-sm ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>
              {t('verifyPatronDesc')}
            </p>
            <p className="text-xs text-[#85BB65] mb-2 sm:mb-3 italic">
              "{t('verifyPatronQuote')}" - Manifesto
            </p>
            <button onClick={checkDiscountEligibility} disabled={isCheckingDiscount} className={`w-full bg-[#85BB65] hover:bg-[#228D57] disabled:${isDark ? 'bg-[#3E3E3C]' : 'bg-gray-200'} ${isDark ? 'text-[#3E3E3C]' : 'text-white'} font-medium py-2.5 px-3 rounded-lg transition-all duration-200 flex items-center justify-center gap-2 text-sm h-12`}>
              {isCheckingDiscount ? (
                <>
                  <div className="animate-spin rounded-full h-4 w-4 border-b-2 border-black"></div>
                  <span className="truncate">{t('verifyingStatus')}</span>
                </>
              ) : (
                <>
                  <ShieldCheckIcon className="w-4 h-4 flex-shrink-0" />
                  <span className="truncate">{t('verifyPatronBtn')}</span>
                </>
              )}
            </button>
          </div>
        )}

        {/* Discount Status Display */}
        {discountChecked && (
          <div className={`p-3 sm:p-4 rounded-lg border ${hasDiscount ? `${isDark ? 'bg-[#85BB65]/20' : 'bg-[#85BB65]/10'} border-[#85BB65]` : `${isDark ? 'bg-[#3E3E3C]' : 'bg-red-50'} border-red-500`}`}>
            <div className="flex items-center gap-2 mb-2">
              {hasDiscount ? (
                <>
                  <ShieldCheckIcon className="w-4 h-4 sm:w-5 sm:h-5 text-[#85BB65] flex-shrink-0" />
                  <span className="text-[#85BB65] font-medium text-sm sm:text-base">Status de Holder Cultural: ATIVO</span>
                </>
              ) : (
                <>
                  <LockIcon className="w-4 h-4 sm:w-5 sm:h-5 text-red-400 flex-shrink-0" />
                  <span className="text-red-400 font-medium text-sm sm:text-base">Status: Inativo</span>
                </>
              )}
            </div>
            <p className={`text-xs sm:text-sm ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>
              {hasDiscount ? t('patronActive') : t('patronInactive')}
            </p>
            {hasDiscount && (
              <div className="flex items-center gap-1 mt-2">
                <UnlockIcon className="w-3 h-3 text-[#85BB65] flex-shrink-0" />
                <p className="text-xs text-[#85BB65] italic">
                  "Libertando mentes passando a visão"
                </p>
              </div>
            )}
          </div>
        )}

        {/* Wallet Purchase Method */}
        {purchaseMethod === 'wallet' ? (
          <>
            <div>
              <label className="block text-xs sm:text-sm font-medium text-[#E8E9C9] mb-2">
                Quantidade em SOL
              </label>
              <input
                type="number"
                value={solAmount}
                onChange={(e) => setSolAmount(e.target.value)}
                placeholder="0.1"
                step="0.01"
                min="0"
                className={`w-full ${isDark ? 'bg-[#3E3E3C] text-[#E8E9C9] placeholder-[#E8E9C9]/50' : 'bg-white text-[#3E3E3C] placeholder-[#3E3E3C]/50'} border border-[#85BB65]/30 rounded-lg px-3 sm:px-4 py-2.5 sm:py-3 focus:outline-none focus:border-[#85BB65] focus:ring-1 focus:ring-[#85BB65] text-sm sm:text-base`}
              />
            </div>
            <button
              onClick={handleEndorse}
              disabled={isLoading}
              className={`w-full bg-[#85BB65] hover:bg-[#228D57] disabled:${isDark ? 'bg-[#3E3E3C]' : 'bg-gray-300'} ${isDark ? 'text-[#3E3E3C]' : 'text-white'} font-medium py-2.5 px-3 rounded-lg transition-all duration-200 disabled:cursor-not-allowed flex items-center justify-center gap-2 text-sm h-12`}
            >
              {isLoading ? (
                <>
                  <div className="animate-spin rounded-full h-4 w-4 border-b-2 border-[#3E3E3C]"></div>
                  <span className="truncate">Comprando $CHAINS...</span>
                </>
              ) : (
                <>
                  <SparklesIcon className="w-4 h-4 flex-shrink-0" />
                  <span className="truncate">Comprar $CHAINS</span>
                </>
              )}
            </button>
          </>
        ) : (
          // PIX Purchase Method
          <>
            <div>
              <label className="block text-xs sm:text-sm font-medium text-[#E8E9C9] mb-2">
                Valor em Reais (R$)
              </label>
              <div className="grid grid-cols-3 gap-2 mb-3">
                <button onClick={() => setPixAmount('50')} className={`px-3 py-2 text-sm rounded border transition-all ${pixAmount === '50' ? 'bg-[#85BB65]/20 border-[#85BB65] text-[#85BB65]' : 'border-[#85BB65]/30 text-[#E8E9C9] hover:border-[#85BB65]'}`}>
                  R$ 50
                </button>
                <button onClick={() => setPixAmount('100')} className={`px-3 py-2 text-sm rounded border transition-all ${pixAmount === '100' ? 'bg-[#85BB65]/20 border-[#85BB65] text-[#85BB65]' : 'border-[#85BB65]/30 text-[#E8E9C9] hover:border-[#85BB65]'}`}>
                  R$ 100
                </button>
                <button onClick={() => setPixAmount('250')} className={`px-3 py-2 text-sm rounded border transition-all ${pixAmount === '250' ? 'bg-[#85BB65]/20 border-[#85BB65] text-[#85BB65]' : 'border-[#85BB65]/30 text-[#E8E9C9] hover:border-[#85BB65]'}`}>
                  R$ 250
                </button>
              </div>
              <input
                type="number"
                value={pixAmount}
                onChange={(e) => setPixAmount(e.target.value)}
                placeholder="100"
                step="10"
                min="10"
                className={`w-full ${isDark ? 'bg-[#3E3E3C] text-[#E8E9C9] placeholder-[#E8E9C9]/50' : 'bg-white text-[#3E3E3C] placeholder-[#3E3E3C]/50'} border border-[#85BB65]/30 rounded-lg px-3 sm:px-4 py-2.5 sm:py-3 focus:outline-none focus:border-[#85BB65] focus:ring-1 focus:ring-[#85BB65] text-sm sm:text-base`}
              />
            </div>
            {onRampStep === 'select' && (
              <button
                onClick={handleOnRampPurchase}
                disabled={!pixAmount || parseFloat(pixAmount) < 50}
                className={`w-full bg-[#85BB65] hover:bg-[#228D57] disabled:${isDark ? 'bg-[#3E3E3C] text-[#E8E9C9]/40' : 'bg-gray-300 text-[#3E3E3C]/40'} ${isDark ? 'text-[#3E3E3C]' : 'text-white'} font-medium py-2.5 px-3 rounded-lg transition-all duration-200 disabled:cursor-not-allowed flex items-center justify-center gap-2 text-sm h-12`}
              >
                <svg className="w-4 h-4 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M3 10h18M7 15h1m4 0h1m-7 4h12a3 3 0 003-3V8a3 3 0 00-3-3H6a3 3 0 00-3 3v8a3 3 0 003 3z" /></svg>
                <span className="truncate">Continuar com {onRampProviders.find(p => p.id === selectedProvider)?.name}</span>
              </button>
            )}
          </>
        )}

        {/* Crypto Estimation for PIX */}
        {pixAmount && purchaseMethod === 'pix' && onRampStep === 'select' && (
          <CryptoEstimation
            pixAmount={pixAmount}
            estimatedCrypto={estimatedCrypto}
            selectedCrypto={selectedCrypto}
            setSelectedCrypto={setSelectedCrypto}
            conversionRate={conversionRate}
            isDark={isDark}
          />
        )}

        {/* Estimated CHAINS for Wallet Purchase */}
        {solAmount && purchaseMethod === 'wallet' && tokenData?.price && (
          <div className={`${isDark ? 'bg-[#3E3E3C]' : 'bg-[#85BB65]/10'} border border-[#85BB65]/30 rounded-lg p-3`}>
            <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'} mb-1`}>Estimativa de $CHAINS recebidos:</div>
            <div className="text-sm text-[#85BB65] font-medium">
              {(parseFloat(solAmount) * 200 / (tokenData.price * 200)).toLocaleString()} $CHAINS
            </div>
            <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'} mt-1`}>
              *Valor aproximado baseado no preço atual
            </div>
          </div>
        )}

        {/* Wallet Not Connected Message */}
        {!userWallet && purchaseMethod === 'wallet' && (
          <div className="space-y-3">
            <button className={`w-full border-2 border-[#85BB65] ${isDark ? 'bg-[#85BB65]/10 hover:bg-[#85BB65]/20 text-[#E8E9C9]' : 'bg-[#85BB65]/5 hover:bg-[#85BB65]/10 text-[#3E3E3C]'} font-medium py-2.5 px-3 rounded-lg transition-all duration-200 flex items-center justify-center gap-2 text-sm h-12`}>
              <LinkIcon className="w-4 h-4 flex-shrink-0" />
              <span className="truncate">Conecte-se ao Protocolo • Liberte sua Carteira</span>
            </button>
            <div className={`${isDark ? 'bg-[#3E3E3C]' : 'bg-[#85BB65]/10'} border border-[#85BB65]/30 rounded-lg p-3`}>
              <div className="text-xs text-[#85BB65] font-medium mb-1">Wallets Solana:</div>
              <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/80' : 'text-[#3E3E3C]/80'}`}>Phantom • Solflare • Backpack</div>
              <div className={`text-xs ${isDark ? 'text-[#E8E9C9]/60' : 'text-[#3E3E3C]/60'} mt-1`}>MetaMask não compatível ⚠️</div>
            </div>
          </div>
        )}

        {/* On-Ramp Flow */}
        {purchaseMethod === 'pix' && onRampStep === 'select' && (
          <OnRampProviderSelection
            providers={onRampProviders}
            selectedProvider={selectedProvider}
            setSelectedProvider={setSelectedProvider}
            isDark={isDark}
          />
        )}
        {purchaseMethod === 'pix' && onRampStep === 'payment' && (
          <OnRampPayment
            provider={onRampProviders.find(p => p.id === selectedProvider)}
            pixAmount={pixAmount}
            estimatedCrypto={estimatedCrypto}
            selectedCrypto={selectedCrypto}
            setSelectedCrypto={setSelectedCrypto}
            conversionRate={conversionRate}
            simulateOnRampPayment={simulateOnRampPayment}
            setOnRampStep={setOnRampStep}
            isDark={isDark}
          />
        )}
        {purchaseMethod === 'pix' && onRampStep === 'processing' && <OnRampProcessing isDark={isDark} />}
        {purchaseMethod === 'pix' && onRampStep === 'complete' && <OnRampComplete estimatedCrypto={estimatedCrypto} selectedCrypto={selectedCrypto} isDark={isDark} />}

        {purchaseMethod === 'pix' && onRampStep === 'select' && (
          <div className={`${isDark ? 'bg-[#85BB65]/10' : 'bg-[#85BB65]/10'} border border-[#85BB65]/30 rounded-lg p-3 text-xs ${isDark ? 'text-[#E8E9C9]/80' : 'text-[#3E3E3C]/80'}`}>
            <div className="flex items-center gap-2 mb-1">
              <CheckCircleIcon className="w-3 h-3 text-[#85BB65]" />
              <span className="text-[#85BB65] font-medium">Sistema On-Ramp Integrado</span>
            </div>
            <div>• Conversão direta BRL → USDC/SOL via parceiros globais</div>
            <div>• KYC simplificado e conformidade regulatória</div>
            <div>• Receba crypto direto na sua carteira Solana</div>
          </div>
        )}

        {/* Testnet Logs */}
        {testnetLogs.length > 0 && (
          <div className={`${isDark ? 'bg-[#3E3E3C]' : 'bg-[#85BB65]/8'} border border-[#85BB65]/20 rounded-lg p-4`}>
            <div className="flex items-center gap-2 mb-3">
              <div className="w-2 h-2 bg-orange-400 rounded-full animate-pulse"></div>
              <span className="text-orange-400 font-medium text-sm">Log Testnet</span>
            </div>
            <div className="space-y-1 text-xs font-mono max-h-32 overflow-y-auto">
              {testnetLogs.map((log, index) => (
                <div key={index} className="flex items-center gap-2">
                  <span className="text-orange-400/60">{log.timestamp}</span>
                  <span className={`${log.type === 'success' ? 'text-[#85BB65]' : log.type === 'error' ? 'text-red-400' : log.type === 'warning' ? 'text-yellow-400' : isDark ? 'text-[#E8E9C9]/70' : 'text-[#3E3E3C]/70'}`}>
                    {log.message}
                  </span>
                </div>
              ))}
            </div>
          </div>
        )}

        {/* Status Message */}
        {status && (
          <div className={`text-center text-sm p-3 rounded-lg flex items-center justify-center gap-2 ${status.includes('ATIVO') || status.includes('Sucesso') || status.includes('Success') || status.includes('confirmado') || status.includes('adquiridos') ? 'bg-[#85BB65]/20 text-[#85BB65] border border-[#85BB65]' : status.includes('Erro') || status.includes('Inativo') ? 'bg-[#3E3E3C] text-red-400 border border-red-700' : 'bg-[#3E3E3C] text-[#85BB65] border border-[#85BB65]'}`}>
            {status.includes('ATIVO') || status.includes('Sucesso') || status.includes('Success') || status.includes('confirmado') || status.includes('adquiridos') ? <SparklesIcon className="w-4 h-4" /> : status.includes('Erro') || status.includes('Inativo') ? <XIcon className="w-4 h-4" /> : null}
            {status}
          </div>
        )}

        {/* Success Message */}
        {(status.includes('confirmado') || status.includes('enviado') || status.includes('adquiridos')) && (
          <div className="bg-[#85BB65]/10 border border-[#85BB65]/30 rounded-lg p-3 text-center">
            <div className={`flex items-center justify-center gap-2 text-xs ${isDark ? 'text-[#E8E9C9]/80' : 'text-[#3E3E3C]/80'} mb-2`}>
              <PartyIcon className="w-4 h-4 text-[#85BB65]" />
              <span>Bem-vindo à Vanguarda Cultural!</span>
            </div>
            <div className="text-xs text-[#85BB65] italic">
              "Você acaba de financiar a próxima onda de criadores."
            </div>
          </div>
        )}
      </div>
    </div>
  );
}

