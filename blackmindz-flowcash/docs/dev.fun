      setIsProcessing(false);
      setTimeout(() => setStatus(''), 8000); // Clear status after a few seconds
    }
  };

  const currentAvailable = availableAmount - claimedAmount;

  return (
    <div className="space-y-6">
      <div className="flex flex-col sm:flex-row sm:items-center justify-between gap-4">
        <h1 className={`text-2xl font-bold ${isDark ? 'text-[#E8E9C9]' : 'text-[#3E3E3C]'}`}>Claims & Recompensas</h1>
